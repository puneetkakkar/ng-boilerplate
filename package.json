{
  "name": "ng-boilerplate",
  "author": "Puneet Kakkar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "prettier --write '{src,e2e}/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "remove:dist": "rimraf dist",
    "build:client:prod": "yarn remove:dist && yarn build --configuration production",
    "build:client:stage": "yarn remove:dist && yarn build --configuration staging",
    "build:client:dev": "yarn remove:dist && yarn build --configuration dev",
    "build:prod": "yarn remove:dist && yarn build:client-and-server-bundles:prod && yarn compile:server",
    "build:stage": "yarn remove:dist && yarn build:client-and-server-bundles:staging && yarn compile:server",
    "serve:client:prod": "ng serve --configuration production",
    "serve:client:stage": "ng serve --configuration staging",
    "serve:client:dev": "ng serve --configuration dev",
    "serve:client:hmr": "ng serve --configuration hmr -- --hmr",
    "build:client-and-server-bundles:prod": "yarn build:client:prod && ng run ng-boilerplate:server:production --bundleDependencies all",
    "build:client-and-server-bundles:staging": "yarn build:client:stage && ng run ng-boilerplate:server:staging --bundleDependencies all",
    "universal": "yarn build:ssr && yarn serve:ssr",
    "compile:server_bak": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr_bak": "yarn build:client-and-server-bundles && yarn compile:server",
    "serve:ssr_bak": "node dist/server",
    "dev:ssr": "ng run ng-boilerplate:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run ng-boilerplate:server:production",
    "prerender": "ng run ng-boilerplate:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/platform-server": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@nguniversal/express-engine": "^9.0.0",
    "@ngx-toolkit/device": "^8.0.0",
    "express": "^4.15.2",
    "rimraf": "^3.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@ngrx/schematics": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "@nguniversal/builders": "^9.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^3.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5",
    "webpack-cli": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  }
}